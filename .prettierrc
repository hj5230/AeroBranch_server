{
    "$schema": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "printWidth": 80,
    "tabWidth": 4,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "proseWrap": "always",
    "htmlWhitespaceSensitivity": "strict"
}
